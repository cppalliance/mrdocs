{{#if isUsing~}}
    {{#if template}}{{>template/head template}}
    {{/if~}}
    using {{name}}{{#if attributes}} {{>symbol/attributes symbol}}{{/if}} = {{>type/declarator type}}
{{~else~}}
    {{#if attributes}}{{>symbol/attributes symbol}}
    {{/if~}}
    typedef {{>type/declarator type decl-name=name}}
{{~/if}};